{
  "name": "ExpressWebpack",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "engines": {
    "node":"10.3.0"
  },
  "scripts": {
    "dev": "nodemon --exec 'babel-node' server",
    "clean": "rm -rf build/ && mkdir build",
    "public":"cp -R ./public ./build/",
    "build": "yarn clean && babel -d ./build *.js routes/*.js controllers/*.js -s && yarn public",
    "start": "node ./build/server.js"
  },
  "dependencies": {
    "async": "^2.6.1",
    "express": "^4.16.3",
    "jwt-simple": "^0.5.1",
    "mongoose": "^5.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5"
  }
}
